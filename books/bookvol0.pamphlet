\spadcommand{n3 := d3 }
$$
{2 \  z \  {x \sp 2}} -{2 \  {y \sp 2}} -x 
$$
\returnType{Type: HomogeneousDistributedMultivariatePolynomial([z,y,x],Fraction Integer)}

