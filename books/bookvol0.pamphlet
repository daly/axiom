Type: HomogeneousDistributedMultivariatePolynomial([z,y,x],Fraction Integer)}
\begin{verbatim}
  n2 := d2
\end{verbatim}
$$
{2 \ z \ {y^2}}+{4 \ x}+1
$$
\returnType{
Type: HomogeneousDistributedMultivariatePolynomial([z,y,x],Fraction Integer)}
\begin{verbatim}
  n3 := d3
\end{verbatim}
$$
{2 \ z \ {x^2}} -{2 \ {y^2}} -x
$$
\returnType{
Type: HomogeneousDistributedMultivariatePolynomial([z,y,x],Fraction Integer)}
