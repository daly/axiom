\newitem\smath{\mbox{\bf product}\opLeftPren{}f(n), 
\allowbreak{} n\opRightPren{}} returns the formal product
\smath{P(n)} verifying \smath{{P(n+1)/P(n)} = f(n)}.
\newitem\smath{\mbox{\bf product}\opLeftPren{}s, 
