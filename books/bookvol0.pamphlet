%Original Page 289

\spadcommand{Km := FFCGX(K,m) }
$$
\mbox{\rm FiniteFieldCyclicGroupExtension(PrimeField 3,4)} 
$$
\returnType{Type: Domain}

\spadcommand{Kn := FFNBX(K,n) }
