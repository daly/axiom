(defun |bcGenEquations| (alist)
 (let (right left y eqnlist)
  (setq y alist)
  (loop while y do
    (setq right (elt (car y) 1))
    (setq y (cdr y))
    (setq left (elt (car y) 1))
    (setq y (cdr y))
    (setq eqnlist (cons (concat left " = " right) eqnlist)))
  (if (cdr eqnlist)
    (|bcwords2liststring| eqnlist)
    (car eqnlist))))

\end{chunk}

