    (|pfWrong|
     (|pfDocument| (list "input stream not exhausted"))
     (|pfListOf| nil)))
   ((null |$stack|)
    (|ncSoftError| (|tokPosn| |$stok|) 
      "System error while parsing, stack is empty." nil)
    (|pfWrong| (|pfDocument| (list "stack empty")) (|pfListOf| nil)))
   (t (car |$stack|)))))

\end{chunk}

