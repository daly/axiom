        (exit
         (seq
          (setq oname (elt ob 2))
          (setq f 
           (seq 
            (when (integerp oname) (exit (eval (gensymmer oname))))
            (exit (symbol-function oname))))
          (when (null (compiled-function-p f))
           (exit (|error| "A required BPI does not exist.")))
