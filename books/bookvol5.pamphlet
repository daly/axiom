\begin{chunk}{defun filterListOfStrings}
(defun |filterListOfStrings| (patterns names)
 (let (result)
 (if (or (null patterns) (null names))
  names
  (dolist (name (reverse names) result)
   (when (|satisfiesRegularExpressions| name patterns)
    (push name result))))))

\end{chunk}

