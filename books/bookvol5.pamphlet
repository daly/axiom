(defun |bcLinearSolveEqnsGen| (htPage)
 (let (vars varlist varString alist eqnString)
  (setq alist (|htpInputAreaAlist| htPage))
  (when (setq vars (|htpLabelInputString| htPage '|unknowns|))
    (setq varlist (|bcString2WordList| vars))
