\begin{chunk}{defun undoCount}
(defun |undoCount| (n)
 "Undo previous n commands"
 (prog (m)
 (declare (special |$IOindex|))
  (return
   (progn
    (setq m 
     (cond 
       ((>= n 0) (- (- |$IOindex| n) 1))
       (t (- n))))
    (cond
