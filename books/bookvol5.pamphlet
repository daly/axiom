(defun |compiledLookupCheck| (op sig dollar)
 (let (fn)
  (setq fn (|compiledLookup| op sig dollar))
  (cond
    ((and (null fn) (eq op '^))
      (setq fn (|compiledLookup| '** sig dollar)))
    ((and (null fn) (eq op '**))
