                  (or (eq (qcar cond) 'and) (eq (qcar cond) '|and|)))
              (|hasSigAnd| (qcdr cond) s0 sl))
            ((and (consp cond) 
                  (or (eq (qcar cond) 'or) (eq (qcar cond) '|or|)))
