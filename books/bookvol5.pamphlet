\begin{chunk}{defun tokConstruct}
(defun |tokConstruct| (head token &rest position)
 (let (result)
  (setq result (cons head token))
  (cond
   ((ifcar position)
    (cond
     ((|pfNoPosition?| (car position)) result)
     (t (|ncPutQ| result '|posn| (car position)) result)))
   (t result))))

\end{chunk}

