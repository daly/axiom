         ((eq op '|braceFromCurly|)
          (if (and (consp argSex) (eq (car argSex) 'seq))
           argSex
           (cons 'seq argSex)))
         ((and (consp op)
               (progn
                (setq qt (car op))
                (setq tmp1 (cdr op))
                (and (consp tmp1) 
                     (eq (cdr tmp1) nil)
                     (progn
                      (setq realOp (car tmp1))
