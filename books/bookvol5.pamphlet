\defun{readSpad2Cmd}{Implement the )read command}
\calls{readSpad2Cmd}{selectOptionLC}
\calls{readSpad2Cmd}{optionError}
\calls{readSpad2Cmd}{pathname}
\calls{readSpad2Cmd}{pathnameTypeId}
