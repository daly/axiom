\begin{chunk}{defun sameUnionBranch 0}
(defun |sameUnionBranch| (uArg m)
 (let (t1 t2 t3)
 (cond
  ((and (consp uArg) 
        (eq (qcar uArg) '|:|)
        (progn
         (setq t1 (qcdr uArg))
         (and (consp t1)
              (progn
               (setq t2 (qcdr t1))
               (and (consp t2)
