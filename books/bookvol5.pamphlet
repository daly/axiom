               (setq tmp1 (cdr t3))
               (and (consp tmp1)
                    (progn
                     (setq rhs (cdr tmp1))
                     t)))
              (setq t1 (cons rhs t1)))))
       (setq t2 (cdr t2))))
      nil |$multiVarPredicateList| nil)))
   (setq predBody
    (cond
     ((cdr |$multiVarPredicateList|)
      (cons 'and
       ((lambda (t4 t5 t6)
        (loop
         (cond
          ((or (atom t5)
               (progn
                (setq t6 (car t5))
                nil))
            (return (nreverse t4)))
          (t
           (and (consp t6)
                (progn
                 (setq tmp1 (cdr t6))
                 (and (consp tmp1)
                      (progn
                       (setq rhs (cdr tmp1))
                       t)))
                      (setq t4
                       (append (reverse (|pvarPredTran| rhs varList))
                               t4)))))
         (setq t5 (cdr t5))))
        nil |$multiVarPredicateList| nil)))
     (t
      (progn
       (setq rhs (cddar |$multiVarPredicateList|))
       (|pvarPredTran| rhs varList)))))
   (dolist (var varList) (push (list 'quote var) result))
   (list '|suchThat| rule
    (cons '|construct| (nreverse result))
    (list 'adef '(|predicateVariable|)
                       '((|Boolean|)
                         (|List| (|Expression| (|Integer|))))
                       '(nil nil) predBody))))))

\end{chunk}

