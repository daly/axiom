   (setq *eof* nil) 
   (setq |$InteractiveMode| t) 
   (setq $boot nil) 
   (setq $newspad t) 
   (setq $spad t) 
   (setq |$e| |$InteractiveFrame|) 
   (|ncIntLoop|)))
