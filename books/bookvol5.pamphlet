   ((< real 0.0)
     (if (< 0.0 imag)
       (conjugate (clngammacase1 real (- imag) (complex real (- imag))))
       (clngammacase1 real imag z)))
   ((< imag 0.0)
     (conjugate (clngammacase23 real (- imag) (complex real (- imag)))))
