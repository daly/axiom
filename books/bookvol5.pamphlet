  (if (or (not (eql (|#| arg) 1)) (null (symbolp (car arg))))
    (|helpSpad2Cmd| '(|savesystem|))
    (progn
     (setq *ThisIsARunningSystem* t)
     (spad-save (symbol-name (car arg))))))

\end{chunk}

