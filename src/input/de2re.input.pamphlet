-- 4 well known differential operators
exp:= dn - 1
sincos := dn**2 + 1
airy := dn**2 - n
hermite := dn**2 - 2*n*dn + 1

-- the recurrences satisfied by the coefficients of their series solutions
--recurrence(exp, 0)
--recurrence(sincos, 0)
--recurrence(airy, 0)
