     </m:mrow>
    </m:mrow>
    <m:mo>=</m:mo>
    <m:mrow>
     <m:munder>
      <m:mo movablelimits="false">lim</m:mo>
      <m:mrow>
       <m:mi>k</m:mi>
       <m:mo>&#x2192;</m:mo>
