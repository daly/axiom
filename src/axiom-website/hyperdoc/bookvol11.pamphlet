<div align="right">
 <m:math display="inline">
  <m:mrow>
   <m:mi>x</m:mi>
   <m:mo>&#x2260;</m:mo>
   <m:mrow>
    <m:mn>0</m:mn>
    <m:mo>,</m:mo>
    <m:mrow>
     <m:mo>-</m:mo>
