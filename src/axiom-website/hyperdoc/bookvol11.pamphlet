       </m:mrow>
       <m:mo>)</m:mo>
      </m:mrow>
      <m:mrow>
       <m:mi mathvariant="normal">B</m:mi>
       <m:mrow>
        <m:mo>(</m:mo>
        <m:mrow>
         <m:mi>a</m:mi>
         <m:mo>,</m:mo>
         <m:mi>b</m:mi>
        </m:mrow>
        <m:mo>)</m:mo>
       </m:mrow>
      </m:mrow>
