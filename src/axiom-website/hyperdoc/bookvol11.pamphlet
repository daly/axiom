         </m:mrow>
         <m:mo>+</m:mo>
         <m:mrow>
          <m:mi mathvariant="normal">&#x221E;</m:mi>
          <m:mspace width="0.2em"/>
          <m:mi mathvariant="normal">&#x2148;</m:mi>
         </m:mrow>
        </m:mrow>
       </m:msubsup>
       <m:mfrac>
        <m:mrow>
         <m:mi>&#x03C0;</m:mi>
