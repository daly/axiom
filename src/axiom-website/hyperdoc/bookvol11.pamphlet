<div align="center">
 <m:math display="block">
  <m:mrow>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mo>|</m:mo>
      <m:mfrac>
       <m:mrow>
        <m:mi mathvariant="normal">&#x0393;</m:mi>
        <m:mrow>
         <m:mo>(</m:mo>
         <m:mi>x</m:mi>
         <m:mo>)</m:mo>
        </m:mrow>
       </m:mrow>
       <m:mrow>
        <m:mi mathvariant="normal">&#x0393;</m:mi>
        <m:mrow>
         <m:mo>(</m:mo>
         <m:mrow>
          <m:mi>x</m:mi>
          <m:mo>+</m:mo>
          <m:mrow>
           <m:mi mathvariant="normal">&#x2148;</m:mi>
           <m:mi>y</m:mi>
          </m:mrow>
         </m:mrow>
         <m:mo>)</m:mo>
        </m:mrow>
       </m:mrow>
      </m:mfrac>
      <m:mo>|</m:mo>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mo>=</m:mo>
    <m:mrow>
     <m:munderover>
      <m:mo movablelimits="false">&#x220F;</m:mo>
      <m:mrow>
       <m:mi>k</m:mi>
       <m:mo>=</m:mo>
       <m:mn>0</m:mn>
      </m:mrow>
      <m:mi mathvariant="normal">&#x221E;</m:mi>
     </m:munderover>
     <m:mo>(</m:mo>
     <m:mrow>
      <m:mn>1</m:mn>
      <m:mo>+</m:mo>
      <m:mfrac>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mn>2</m:mn>
       </m:msup>
       <m:msup>
        <m:mrow>
         <m:mo>(</m:mo>
         <m:mrow>
          <m:mi>x</m:mi>
          <m:mo>+</m:mo>
