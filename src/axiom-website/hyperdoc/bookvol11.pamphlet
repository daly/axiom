        </m:mfrac>
       </m:mrow>
       <m:mo>)</m:mo>
      </m:mrow>
      <m:msup>
       <m:mi mathvariant="normal">&#x2147;</m:mi>
       <m:mrow>
        <m:mo>-</m:mo>
        <m:mfrac bevelled="true">
         <m:mi>z</m:mi>
         <m:mi>k</m:mi>
        </m:mfrac>
       </m:mrow>
      </m:msup>
     </m:mrow>
    </m:mrow>
   </m:mrow>
  </m:mrow>
 </m:math>
