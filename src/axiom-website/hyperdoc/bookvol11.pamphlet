<div align="center">
 <m:math display="block">
  <m:mrow>
   <m:mrow>
    <m:mrow>
     <m:mi mathvariant="normal">&#x0393;</m:mi>
     <m:mrow>
      <m:mo>(</m:mo>
      <m:mi>z</m:mi>
      <m:mo>)</m:mo>
