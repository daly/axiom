         <m:mi mathvariant="normal">&#x2147;</m:mi>
         <m:mrow>
          <m:mo>-</m:mo>
          <m:mi>t</m:mi>
         </m:mrow>
        </m:msup>
       </m:mrow>
       <m:mo>)</m:mo>
      </m:mrow>
      <m:mspace width="0.2em"/>
      <m:mfrac>
       <m:mrow>
        <m:mi mathvariant="normal">&#x2146;</m:mi>
        <m:mi>t</m:mi>
       </m:mrow>
       <m:mi>t</m:mi>
      </m:mfrac>
     </m:mrow>
     <m:mo>-</m:mo>
     <m:mrow>
      <m:msubsup>
       <m:mo>&#x222B;</m:mo>
