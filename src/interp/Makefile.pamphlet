	${DOCUMENT} ${NOISE} debugsys.lisp ; \
	rm -f ${DOC}/debugsys.lisp.pamphlet ; \
	rm -f ${DOC}/debugsys.lisp.tex ; \
	rm -f ${DOC}/debugsys.lisp )

