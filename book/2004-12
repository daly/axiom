	  echo 7 applying toploop patch to unixport/init_gcl.lsp ; \=0A=
	  patch <${SPD}/zips/${GCLVERSION}.unixport.init_gcl.lsp.in.patch )=0A=
@=0A=
<<gcl-2.6.2.toploop.patch>>=3D=0A=
