     reConsts =3D re.MULTILINE+re.DOTALL
+    axiomPattern =3D re.compile(r'^[
\t]*\\begin{axiom}(.*?)\\end{axiom}',reConsts)
=20
     def htmlMarkup(code):
         newCode =3D re.compile(r'&', reConsts).sub(r'&amp;',code)
@@ -26,7 +26,7 @@
         newCode =3D re.compile(r'_', reConsts).sub('&#95;', newCode)
         return newCode
