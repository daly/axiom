diff -Naur AxiomWiki-0.2.0/axiomWrapper.py =
AxiomWiki-0.2.1/axiomWrapper.py
--- AxiomWiki-0.2.0/axiomWrapper.py	2004-07-07 11:27:38.609375000 -0400
+++ AxiomWiki-0.2.1/axiomWrapper.py	2004-07-07 20:05:30.750000000 -0400
@@ -22,15 +22,31 @@
 outputPattern =3D r'(.*?)\n\(\d+\) -> '
 reConsts =3D re.MULTILINE+re.DOTALL
=20
-def renderAxiom(axiomCodeList):
+def renderAxiom(axiomCodeList):
+    def securityCheck(code):
+      newCode =3D re.compile(r'^[ \t]*\)sys([^\n]*)',reConsts).sub(r'-- =
not
allowed: )sys\1\n)sys',code)
+      return newCode
+   =20
     n =3D 1
     unifiedCode =3D ''
-    for axiomCode in axiomCodeList:
-        axiomFileName =3D fileNameFor(axiomCode, 25, '.%3.3d.input'%n)
-        n =3D n + 1
-        unifiedCode =3D unifiedCode + ')read %s\n'%axiomFileName
+    for axiomCode in axiomCodeList:
+        newAxiomCode =3D securityCheck(axiomCode)
+        n =3D n + 1
+        if re.match(r'^\s*\)abbrev',newAxiomCode):
+            # Starts like a package so compile as library code
+            axiomFileName =3D fileNameFor(newAxiomCode, 25, =
'.%3.3d.spad'%n)
+            # This is just for appearances
+            newAxiomCode =3D newAxiomCode +
'------------------------------------------------------------------------=
\n'
+            unifiedCode =3D unifiedCode + ')set message prompt none\n'
+            unifiedCode =3D unifiedCode + ')sys cat %s\n'%axiomFileName
+            unifiedCode =3D unifiedCode + ')compile %s\n'%axiomFileName
+            unifiedCode =3D unifiedCode + ')set message prompt step\n'
+        else:
+            # otherwise it is just an input file
+            axiomFileName =3D fileNameFor(newAxiomCode, 25, =
'.%3.3d.input'%n)
+            unifiedCode =3D unifiedCode + ')read %s\n'%axiomFileName
         axiomFile =3D open(axiomFileName, 'w')
-        axiomFile.write(axiomCode)
+        axiomFile.write(newAxiomCode)
         axiomFile.close()
     try:
         latexCode=3DrunAxiom(unifiedCode,axiomTemplate)




