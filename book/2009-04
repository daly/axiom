 <<layer2>>=
 
 LAYER2=\
-  ${OUT}/API.o      ${OUT}/ASP29.o    \
-  ${OUT}/ATRIG.o    ${OUT}/ATRIG-.o   ${OUT}/BMODULE.o  ${OUT}/CACHSET.o  \
