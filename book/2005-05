PS : Original gcl-devel mailing message
(mflop.lisp from Nicolas Neuss (Nicolas.Neuss@iwr.uni-heidelberg.de)):
http://lists.gnu.org/archive/html/gcl-devel/2004-07/msg00080.html

------=_NextPart_000_0000_01C56140.7E374600
Content-Type: application/octet-stream;
	name="mflop.lisp"
Content-Transfer-Encoding: quoted-printable
Content-Disposition: attachment;
	filename="mflop.lisp"

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;=0A=
;;;;  mflop.lisp=0A=
;;;;  (C) Nicolas Neuss (Nicolas.Neuss@iwr.uni-heidelberg.de)=0A=
;;;;  mflop.lisp is in the public domain.=0A=
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;=0A=
=0A=
(defconstant +N-long+ #x100000)  ; does not fit in secondary cache=0A=
(defconstant +N-short+ #x100)    ; fits in primary cache=0A=
=0A=
(defparameter *mflop-delta* 5.0=0A=
  "Time interval in seconds over which we measure performance.")=0A=
=0A=
(defun make-double-float-array (size &optional (initial 0.0d0))=0A=
   (make-array size :element-type 'double-float :initial-element =
initial))=0A=
=0A=
(defun ddot (x y n)=0A=
  (declare (type fixnum n)=0A=
	   (type (simple-array double-float (*)) x y))=0A=
  (declare (optimize (safety 0) (space 0) (debug 0) (speed 3)))=0A=
  (loop for i of-type fixnum from 0 below n=0A=
	summing (* (aref x i) (aref y i)) double-float))=0A=
=0A=
(defun daxpy (x y n)=0A=
  (declare (type fixnum n)=0A=
	   (type (simple-array double-float (*)) x y))=0A=
  (declare (optimize (safety 0) (space 0) (debug 0) (speed 3)))=0A=
  (loop with s of-type double-float =3D 0.3d0=0A=
	for i of-type fixnum from 0 below n do=0A=
	(incf (aref y i) (* s (aref x i)))))=0A=
=0A=
(defun test (fn name size)=0A=
  (let ((x (make-double-float-array +N-long+))=0A=
	(y (make-double-float-array +N-long+)))=0A=
    (format=0A=
     t "~A-~A: ~$ MFLOPS~%"=0A=
     name=0A=
     (if (=3D size +N-long+) "long" "short")=0A=
     (loop with after =3D 0=0A=
	   for before =3D (get-internal-run-time) then after=0A=
	   and count =3D 1 then (* count 2)=0A=
	   do=0A=
	   (loop repeat count do (funcall fn x y size))=0A=
	   (setq after (get-internal-run-time))=0A=
	   (when (> (/ (- after before) internal-time-units-per-second)=0A=
		    *mflop-delta*)=0A=
	     (return (/ (* 2 size count internal-time-units-per-second)=0A=
			(* 1e6 (- after before)))))))))=0A=
=0A=
(defun mflop-test ()=0A=
  "Returns several numbers characteristic for the efficiency with which=0A=
your CL implementation will process numeric code written in a C/Fortran=0A=
style.  This results should be significant also for other code using =
arrays=0A=
for achieving good data locality.  Please compare these numbers to those=0A=
obtained by the C version in mflop.c."=0A=
  (test #'ddot 'ddot +N-long+)=0A=
  (test #'ddot 'ddot +N-short+)=0A=
  (test #'daxpy 'daxpy +N-long+)=0A=
  (test #'daxpy 'daxpy +N-short+))=0A=
=0A=
(mflop-test)
------=_NextPart_000_0000_01C56140.7E374600--





