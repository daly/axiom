(defun foo (s) =0A=
  (let* ((get (read s nil 'eof)) =0A=
         (fn (and (eq get 'get) (string-downcase (read s nil =
'eof))))=0A=
         (fn (when (probe-file fn) fn)))=0A=
    (format s "HTTP/1.1 ~S~%" (if fn 404 500))=0A=
    (format s "Content-type: text/html~%~%")=0A=
    (format t "get ~a fn ~a~%" get fn)=0A=
    (when fn=0A=
      (if (pathname-name (pathname fn))=0A=
          (with-open-file (q fn) (si::copy-stream q s))=0A=
        (dolist (l (directory fn))=0A=
          (format s "<a href=3D\"~a\">~a</a> <a href=3D\"~a/\"> /... =
</a><br>~%"=0A=
	    (namestring l) (namestring l) (namestring l)))=0A=
	    ))=0A=
    (close s))=0A=
)=0A=
=0A=
(defun bar (p fn) =0A=
  (let ((s (si::socket p :server fn))) =0A=
        (tagbody l =0A=
                (when (si::listen s) =0A=
                        (let ((w (si::accept s))) =0A=
                                (foo w))) =0A=
                (sleep 3) =0A=
                (go l))))=0A=
=0A=
(bar 8085 #'foo)=0A=
=0A=

