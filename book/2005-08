        getMatrixFraction(polytmpfile)$ReadFile

--  Combinatorics
   
      nVertices s ==
        polyprop(s, "N__VERTICES")
        getInteger(polytmpfile)$ReadFile :: Cardinal

      nBoundedVertices s ==
        polyprop(s, "N__BOUNDED__VERTICES")
        getInteger(polytmpfile)$ReadFile :: Cardinal

