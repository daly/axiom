      nPoints s ==
        polyprop(s, "N__POINTS")
        getInteger(polytmpfile)$ReadFile :: Cardinal

      nInequalities s ==
        polyprop(s, "N__INEQUALITIES")
        getInteger(polytmpfile)$ReadFile :: Cardinal

