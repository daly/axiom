\section{Statistics}
\begin{verbatim}
 
initial (1) -> -- ----------[ A x i o m ]----------
-- ---------- Initialization ----------
)set messages autoload off
 
)set messages time on
 
)set quit unprotected
 
-- ---------- Statistics ----------
-- Compute the mean of a list of numbers => 9
mean(lst) == reduce(+, lst)/#lst
 
                                                                   Type: Void
                                                                  Time: 0 sec
mean([3, 7, 11, 5, 19])
 
