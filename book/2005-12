                       matrix([[A(1,3), A(1,1), A(1,4)], _
                               [A(2,3), A(2,1), A(2,4)], _
                               [A(4,3), A(4,1), A(4,4)]])), _
           horizConcat(A, vertConcat(subMatrix(A, 1, 2, 3, 4), _
                                     matrix([[A(4,3), A(4,2)], _
                                             [A(1,3), A(1,2)]]))))
 

