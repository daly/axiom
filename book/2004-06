          (* (+ (* - 6 (** x 2)) (+ (* 2 b) (* 2 a))) (** y 2)))

        (* (+ (* - 4 (** x 3)) (+ (+ (** b 2) (* (* 2 a) b)) (** a 2))) y))

      (+ (* - 1 (** x 4)) 1))
     )
