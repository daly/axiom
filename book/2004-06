/bin/sh ./libtool --mode=3Dcompile gcc -DHAVE_CONFIG_H -I. -I. -I. -D__GMP_=
WITHIN_GMP    -g -O2 -fomit-frame-pointer -mcpu=3Dpentiumpro -march=3Di486 =
