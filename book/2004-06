s/clrbit.o
gcc -DHAVE_CONFIG_H -I. -I. -I.. -D__GMP_WITHIN_GMP -I.. -g -O2 -fomit-fram=
e-pointer -mcpu=3Dpentiumpro -march=3Di486 -c clrbit.c -o clrbit.o >/dev/nu=
ll 2>&1
/bin/sh ../libtool --mode=3Dcompile gcc -DHAVE_CONFIG_H -I. -I. -I.. -D__GM=
P_WITHIN_GMP -I..    -g -O2 -fomit-frame-pointer -mcpu=3Dpentiumpro -march=
=3Di486 -c -o cmp.lo `test -f cmp.c || echo './'`cmp.c
