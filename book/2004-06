=3Di486 -c -o get_d.lo `test -f get_d.c || echo './'`get_d.c
gcc -DHAVE_CONFIG_H -I. -I. -I.. -D__GMP_WITHIN_GMP -I.. -g -O2 -fomit-fram=
e-pointer -mcpu=3Dpentiumpro -march=3Di486 -c get_d.c  -fPIC -DPIC -o .libs=
/get_d.o
gcc -DHAVE_CONFIG_H -I. -I. -I.. -D__GMP_WITHIN_GMP -I.. -g -O2 -fomit-fram=
e-pointer -mcpu=3Dpentiumpro -march=3Di486 -c get_d.c -o get_d.o >/dev/null=
 2>&1
/bin/sh ../libtool --mode=3Dcompile gcc -DHAVE_CONFIG_H -I. -I. -I.. -D__GM=
P_WITHIN_GMP -I..    -g -O2 -fomit-frame-pointer -mcpu=3Dpentiumpro -march=
=3Di486 -c -o get_d_2exp.lo `test -f get_d_2exp.c || echo './'`get_d_2exp.c
gcc -DHAVE_CONFIG_H -I. -I. -I.. -D__GMP_WITHIN_GMP -I.. -g -O2 -fomit-fram=
e-pointer -mcpu=3Dpentiumpro -march=3Di486 -c get_d_2exp.c  -fPIC -DPIC -o =
=2Elibs/get_d_2exp.o
gcc -DHAVE_CONFIG_H -I. -I. -I.. -D__GMP_WITHIN_GMP -I.. -g -O2 -fomit-fram=
e-pointer -mcpu=3Dpentiumpro -march=3Di486 -c get_d_2exp.c -o get_d_2exp.o =
>/dev/null 2>&1
