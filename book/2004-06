cat init_pre_gcl.lsp.tmp | sed \
	-e "s,@LI-VERS@,(`cat ../majvers`.`cat ../minvers`) `date`,1" \
	-e "s,@LI-MINVERS@,`cat ../minvers`,1" \
	-e "s,@LI-MAJVERS@,`cat ../majvers`,1" \
	-e "s/@LI-CC@/\"gcc -c -Wall -DVOL=3Dvolatile -fsigned-char -fwritable-str=
ings -pipe \"/1" \
	-e "s,@LI-LD@,\"gcc -o \",1" \
	-e "s,@LI-LD-LIBS@,\"   -lpre_gcl -lm  /usr/lib/libbfd.a /usr/lib/libibert=
y.a -lreadline -lncurses -lc -lgclp /home/magnus/usr/source/axiom/cvs-test/=
axiom/obj/linux/lib/libspad.a \",1" \
	-e "s,@LI-OPT-THREE@,\"-O3 -fomit-frame-pointer\",1" \
	-e "s,@LI-OPT-TWO@,\"-O\",1" \
	-e "s,@LI-INIT-LSP@,\"init_pre_gcl.lsp\",1" >init_pre_gcl.lsp
cp init_pre_gcl.lsp foo
echo " (in-package \"USER\")(system:save-system \"saved_pre_gcl\")" >>foo
/home/magnus/usr/source/axiom/cvs-test/axiom/lsp/gcl-2.6.2/unixport/raw_pre=
_gcl /home/magnus/usr/source/axiom/cvs-test/axiom/lsp/gcl-2.6.2/unixport/ -=
