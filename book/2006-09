Wed Sep 20 21:32:52 2006
+++ new-axiom--windows--1-1/zips/gcl-2.6.5w.unixport.makefile.patch
Wed Sep 20 21:33:10 2006
@@ -10,12 +10,3 @@
 =20
  ifeq ($(ARRS),)
  ARRS:=3Dar rs
-@@ -28,7 +29,7 @@
- 	rm -rf gmp
- 	mkdir gmp
- 	a=3D"$^" ; \
--	for i in $^ ; do \
-+	for i in $$a ; do \
- 		cp $$i gmp/$$(echo $$i | sed -e 's,\.\./,,1' -e
's,/,_,g') ; \
- 	done
- 	touch $@

