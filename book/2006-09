echo '(compiler::link nil "/home/s/test/tt/axiom7/ax-build/build/x86_64-unk=
nown-linux/bin/lisp" \
                (format nil "(progn (let ((*load-path* (cons ~S *load-path*=
))\
                                          (si::*load-types* ~S)) \
                                         (compiler::emit-fn t)) \
