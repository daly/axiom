- <<Makefile.hp9>>=
- # System dependent Makefile for the HP 9000/730 (700 series) platform
- # Platform variable
- PLF= HP9platform
- # C compiler flags
- CCF=" -D${PLF} -Ae"
- # Loader flags
- LDF=
- # C compiler to use
- CC= cc -O
- AWK="/usr/local/bin/gawk -W version -W compat"
- RANLIB=ranlib
- TOUCH=touch
- TAR=/usr/local/bin/gnutar
- AXIOMXLROOT=${AXIOM}/compiler
- O=o
- BYE=bye
- LISP=lisp
- <<GCLOPTS>>
- <<SRCDIRS>>
