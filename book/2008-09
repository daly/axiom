+ (PROG (#1=#:G84725 |q|)
+  (RETURN
+   (SEQ
+    (COND
+     ((|<| |n| 1) (|error| "index out of range"))
+     ((QUOTE T)
+      (SEQ
+       (LETT |p|
+        (SPADCALL |p|
+         (PROG1
+          (LETT #1# (|-| |n| 1) |URAGG-;split!;AIA;32|)
+          (|check-subtype| (|>=| #1# 0) (QUOTE (|NonNegativeInteger|)) #1#))
