+       (LETT |z| (SPADCALL |x| (QREFELT |$| 14)) |URAGG-;cycleSplit!;2A;33|)
+       (SEQ G190
+        (COND 
+         ((NULL
+           (COND
+            ((SPADCALL |z| |y| (QREFELT |$| 36)) (QUOTE NIL))
+            ((QUOTE T) (QUOTE T))))
