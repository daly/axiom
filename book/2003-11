+          fputs("\2latex:$\\displaystyle\n",stdout);
+	  tex_to_latex();
+	  mmode=0; wait_type=1;
+          fputs("$\5\n",stdout);
+       	} else {
+          mathBufferPtr = 0;
+          mathBufferLen = 0;
