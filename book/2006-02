??changed:
-i : MyInteger   := 1
-r0: MyRational  := 1
-r1: MyRational1 := 1
-r2: MyRational2 := 1
i : MyInteger   := 1$MyInteger
)sh MyRational
r0: MyRational  := 1$MyRational
r1: MyRational1 := 1$MyRational1
r2: MyRational2 := 1$MyRational2

