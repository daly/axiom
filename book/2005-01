(defun get-NAG-chapter (chapter function-list)
   (apply 'append
   (mapcar
    #'(lambda (f)
      (cond ((equalp chapter (subseq (string f) 0 (length chapter))) (lis=
t f ))))
    function-list)))

(si::use-fast-links nil)
(get-NAG-chapter ch fl)
