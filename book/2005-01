> (defun get-NAG-chapter (chapter function-list)
>    (apply 'append
>    (mapcar
>     #'(lambda (f)
>       (cond ((equalp chapter (subseq (string f) 0 (length chapter))) (list f 
> ))))
>     function-list)))
> 
> (si::use-fast-links nil)
