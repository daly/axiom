Changes http://page.axiom-developer.org/zope/mathaction/AxiomMail/diff
--
)clear all

-- make three vectors
u : VECTOR INT := new(5,12)
v : VECTOR INT := vector([1,2,3])
w : VECTOR INT := vector([2,3,4])
-- multiply them
cross(v,w)
-- dot product
dot(v,w)
-- ask for the length
#(v)
-- access an element
v.2
-- set an element
v.3 := 99
-- show the vector
v
-- multiply by a constant
5 * v
-- on either side
v * 7
-- add them
v + w
-- substract them
v - w
-- display all possible functions
)show Vector(Integer)



