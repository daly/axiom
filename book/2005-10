+/*
+ *  Treat ~ as space
+ */
+jsMath.Parser.prototype.nextIsSpace =3D function () {
+  return this.string.charAt(this.i) =3D=3D ' ' ||
+         this.string.charAt(this.i) =3D=3D '~';
+}
+jsMath.Parser.prototype.special['~'] =3D 'Space';
+
+/*
+ *  Implement \[ ... \], \( ... \), etc.
+ */
+jsMath.Macro('[','\\left[');  jsMath.Macro(']','\\right]');
+jsMath.Macro('(','\\left(');  jsMath.Macro(')','\\right)');
+jsMath.Macro('<','\\left<');  jsMath.Macro('>','\\right>');
+// can't do \. in a reasonable way
+jsMath.Parser.prototype.macros['|'] =3D ['HandleLR','|','|'];
+jsMath.Parser.prototype.macros['=3D'] =3D ['HandleLR','\\|','\\|'];
+
+/*
+ *  Make non-standard \left{ and \right} work
+ */
+jsMath.Parser.prototype.delimiter['}'] =3D [5,2,0x67,3,0x09];
+jsMath.Parser.prototype.delimiter['{'] =3D [4,2,0x66,3,0x08];
+
+
+/*
+ *  Immitate mimeTeX \big... and \Big... ops
+ */
+
+// make the normal ones in text mode
+jsMath.Macro('int','\\intop\\nolimits');
+jsMath.Macro('oint','\\ointop\\nolimits');
+jsMath.Macro('sum','\\sumop\\nolimits');
+jsMath.Macro('prod','\\prodop\\nolimits');
+jsMath.Macro('coprod','\\coprodop\\nolimits');
+
+jsMath.Macro('bigint','\\bigintop\\nolimits');=20
+jsMath.Macro('bigoint','\\bigointop\\nolimits');
+jsMath.Macro('bigsum','\\bigsumop\\nolimits');
+jsMath.Macro('bigprod','\\bigprodop\\nolimits');
+jsMath.Macro('bigcoprod','\\bigcoprodop\\nolimits');
+
+jsMath.Macro('Bigint','\\bigintop\\limits');
+jsMath.Macro('Bigoint','\\bigointop\\limits');
+jsMath.Macro('Bigsum','\\bigsumop\\limits');
+jsMath.Macro('Bigprod','\\bigprodop\\limits');
+jsMath.Macro('Bigcoprod','\\bigcoprod\\limits');
+
+/*
+ *  The characters needed for the macros above
+ */
+jsMath.Parser.prototype.mathchardef['coprodop'] =3D [1,3,0x60];
+jsMath.Parser.prototype.mathchardef['prodop']   =3D [1,3,0x51];
+jsMath.Parser.prototype.mathchardef['sumop']    =3D [1,3,0x50];
+
+jsMath.Parser.prototype.mathchardef['bigintop']    =3D [1,3,0x5A];
+jsMath.Parser.prototype.mathchardef['bigointop']   =3D [1,3,0x49];
+jsMath.Parser.prototype.mathchardef['bigcoprodop'] =3D [1,3,0x61];
+jsMath.Parser.prototype.mathchardef['bigprodop']   =3D [1,3,0x59];
+jsMath.Parser.prototype.mathchardef['bigsumop']    =3D [1,3,0x58];
+
+/*
+ * Unlink the small versions so they don't enlarge in display mode
+ */
+jsMath.TeX['cmex10'][0x48].n =3D null;
+jsMath.TeX['cmex10'][0x50].n =3D null;
+jsMath.TeX['cmex10'][0x51].n =3D null;
+jsMath.TeX['cmex10'][0x52].n =3D null;
+jsMath.TeX['cmex10'][0x60].n =3D null;
+
+
+/*
+ *  Some other missing items
+ */
+jsMath.Macro('/','{}'); // insert an empty box \/
+jsMath.Macro('raisebox','\\raise #1px ',1); // convert to \raise
+jsMath.Macro('hfill','\\quad ',1); // punt
+jsMath.Macro('fbox','\\oldfbox{$#1$}',1); // do fbox in math mode
+
+/*
+ *  These get new JavaScript routines
+ */
+jsMath.Parser.prototype.macros['unitlength'] =3D 'unitlength';
+jsMath.Parser.prototype.macros['hspace']     =3D 'hspace';
+jsMath.Parser.prototype.macros['fs']         =3D 'fs';
+jsMath.Parser.prototype.macros['oldfbox']    =3D 'FBox';
+
+/*
+ *  Add some JavaScript functions to the parser
+ */
+jsMath.Package(jsMath.Parser,{
+ =20
+  /*
+   *  Implement \left x ... \right x
+   */
+  HandleLR: function (name,data) {
+    var arg =3D this.GetUpto(name,name); if (this.error) return;
+    this.string =3D '\\left'+data[0]+arg+'\\right'+data[1];
+    this.i =3D 0;
+  },
+
+  /*
+   *  Hold the unit length in mlist.data
+   */
+  unitlength: function (name) {
+    var n =3D this.GetArgument(this.cmd+name); if (this.error) return;
+    if (!n.match(/^-?(\d+(\.\d*)?|\.\d+)$/)) {
+      this.Error("Argument for "+this.cmd+name+" must be a number");
+      return;
+    }
+    this.mlist.data['unitlength'] =3D n;
+  },
+
+  /*
+   *  Get the length (converted to ems) and multiply by the unit length
+   */
+  hspace: function (name) {
+    var w =3D this.GetArgument(this.cmd+name); if (this.error) return;
+    if (!w.match(/^-?(\d+(\.\d*)?|\.\d+)$/)) {
+      this.Error("Argument for "+this.cmd+name+" must be a number");
+      return;
+    }
+    w /=3D jsMath.em
+    if (this.mlist.data['unitlength']) {w *=3D this.mlist.data['unitlength=
']}
+    this.mlist.Add(jsMath.mItem.Space(w));
+  },
+ =20
+  /*
+   *  Implement \fs{...} for font-size changing
+   */
+  fs: function (name) {
+    var n =3D this.GetArgument(this.cmd+name); if (this.error) return;
+    if (!n.match(/^[-+]?\d+$/)) {
+      this.Error("Argument for "+this.cmd+name+" must be an integer");
+      return;
+    }
+    if (n.match(/[-+]/)) {n =3D n - 0; n +=3D this.mlist.data.size}
+    this.mlist.data.size =3D n =3D Math.max(0,Math.min(9,n));
+    this.mlist.Add(new jsMath.mItem('size',{size: n}));
+  },
+
+  /*
+   *  Repalce the Array function by one that accepts an optional
+   *  parameter for the column types, and that handle's mimeTeX's
+   *  "preamble" format.
+   */
+  Array: function (name,delim) {
+    var columns =3D delim[2]; var cspacing =3D delim[3];
+    if (!columns && this.GetNext() =3D=3D '{') {
+      columns =3D this.GetArgument(this.cmd+'begin{'+name+'}');
+      if (this.error) return;
+    } else {
+      columns =3D '';
+    }
+    columns =3D columns.replace(/[^clr]/g,'');
+    columns =3D columns.split('');
+    var data =3D this.mlist.data;
+    var arg =3D this.GetEnd(name); if (this.error) return;
+    if (arg.match(/\$/)) {arg =3D arg.replace(/^([^$]*)\$/,''); columns =
=3D RegExp.$1}
+    var parse =3D new jsMath.Parser(arg+'\\\\',null,data.size);
+    parse.matrix =3D name; parse.row =3D []; parse.table =3D [];
+    parse.Parse(); if (parse.error) {this.Error(parse); return}
+    parse.HandleRow(name,1);  // be sure the last row is recorded
+    var box =3D jsMath.Box.Layout(data.size,parse.table,columns,cspacing);
+    // Add parentheses, if needed
+    if (delim[0] && delim[1]) {
+      var left  =3D jsMath.Box.Delimiter(box.h+box.d,this.delimiter[delim[=
0]],'T');
+      var right =3D jsMath.Box.Delimiter(box.h+box.d,this.delimiter[delim[=
1]],'T');
+      box =3D jsMath.Box.SetList([left,box,right],data.style,data.size);
+    }
+    this.mlist.Add(jsMath.mItem.Atom((delim[0]? 'inner': 'ord'),box));
+  },
+
+  /*
+   *  Similarly for Matrix (used by \matrix and \array)
+   */
+  Matrix: function (name,delim) {
+    var data =3D this.mlist.data;
+    var arg =3D this.GetArgument(this.cmd+name); if (this.error) return;
+    if (arg.match(/\$/)) {arg =3D arg.replace(/^([^$]*)\$/,''); delim[2] =
=3D RegExp.$1}
+    var parse =3D new jsMath.Parser(arg+'\\\\',null,data.size);
+    parse.matrix =3D name; parse.row =3D []; parse.table =3D [];
+    parse.Parse(); if (parse.error) {this.Error(parse); return}
+    parse.HandleRow(name,1);  // be sure the last row is recorded
+    var box =3D jsMath.Box.Layout(data.size,parse.table,delim[2]);
+    // Add parentheses, if needed
+    if (delim[0] && delim[1]) {
+      var left  =3D jsMath.Box.Delimiter(box.h+box.d,this.delimiter[delim[=
0]],'T');
+      var right =3D jsMath.Box.Delimiter(box.h+box.d,this.delimiter[delim[=
1]],'T');
+      box =3D jsMath.Box.SetList([left,box,right],data.style,data.size);
+    }
+    this.mlist.Add(jsMath.mItem.Atom((delim[0]? 'inner': 'ord'),box));
+  }
+});
