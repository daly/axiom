---BEGIN aaa.spad.pamphlet
<<*>>=
foo(x: Integer): Integer == <<foo body>>
@

<<foo body>>=
z := 1
for i in 1.. x repeat
     <<auxiliary computation>>
     h := <<post computation>>
     <<binding z>>
z
@

<<auxiliary computation>>=
h := 1
for j in 1..i repeat
     h := h + j
     h := 2*h
@

<<post computation>>=
if h > 100
   then
     h-2
   else
     h+2
@

<<binding z>>=
h := 3*h
z := h
@
---END aaa.spad.pamphlet



