Content-Type: application/octet-stream;
	name="unique"
Content-Transfer-Encoding: quoted-printable
Content-Disposition: attachment;
	filename="unique"

#! /usr/bin/perl=0A=
# unique=0A=
# - suppress indentical initial fields in input=0A=
# - assumes input is sorted=0A=
# - repeated lines contain initial blanks where=0A=
# - duplicate=0A=
# Bill Page 14 August 2003=0A=
#=0A=
while (<>) {=0A=
  $new =3D $_;=0A=
  $b =3D 0;=0A=
  for ($i=3D0;$i<length($new) and substr($new,$i,1) eq =
substr($old,$i,1);$i++) {=0A=
    if (substr($new,$i,1) eq " ") {=0A=
      substr($new,0,$i) =3Dsubstr("                                     =
",0,$i);=0A=
      last if ++$b ge 2;=0A=
    };=0A=
  };=0A=
  print "$new";=0A=
  $old =3D $_;=0A=
}; =0A=

------=_NextPart_000_0001_01C36702.088C16E0
Content-Type: application/octet-stream;
	name="depends"
Content-Transfer-Encoding: quoted-printable
Content-Disposition: attachment;
	filename="depends"

#! /usr/bin/perl=0A=
# depends=0A=
# - generate dependency list for algebra/*.spad files=0A=
# Bill Page 14 August 2003=0A=
#=0A=
while ($target =3D shift) {=0A=
  open(IN,"<$target") or die "No files $target\n"; # for each file=0A=
  read(IN,$buf,100000);=0A=
  close(IN);=0A=
  $buf =3D~ s/\n *\n/\n/g;=0A=
  $buf =3D~ s/\n *\+\+[^\n]*//g;=0A=
  $buf =3D~ s/\n *\-\-[^\n]*//g;=0A=
  while ($buf =3D~ s/.*\)abbrev\w* +(\w+) +(\w+) +(\w+)//) {=0A=
    $type =3D $1;=0A=
    $abbrev =3D $2;=0A=
    $module =3D $3;=0A=
    $buf =3D~ s/.*with(([^\n]*\n\ *)[^\n]*(\2[^\n]*)*)//;=0A=
    $with =3D "$1\n"; # for each 'with' clause in the file=0A=
#print "Debug: $type $abbrev $module\n";=0A=
    $pamphlet =3D `grep -l '\)abbrev  *$type  *$abbrev  *$module' =
src/algebra/*.spad.pamphlet`;=0A=
    chop $pamphlet;=0A=
    $name =3D "";=0A=
    $files =3D `fgrep -l -e ' $module ' -e '$module\n' =
int/algebra/*.spad`;=0A=
    $files =3D~ s/\n/ /g;=0A=
    while ($with =3D~ s/ *(\w+) *\:[^\n]*\n//) {=0A=
      $name =3D $1; # for each 'exported' function=0A=
      open(IN,"grep -l '$name\ *[^\:\ ]' $files |");=0A=
      while (<IN>) {=0A=
        chop;=0A=
        $spad =3D $_;=0A=
# Only include those cross-references that contain the same module name=0A=
# except if they are inside comments=0A=
        $spad =3D~ s/int\/algebra\///;=0A=
        $target =3D~ s/int\/algebra\///;=0A=
        if ($spad ne $target) {=0A=
          print "$spad uses $name from $module in $target ($pamphlet)\n";=0A=
        };=0A=
      };=0A=
      close(IN);=0A=
    };=0A=
  };=0A=
};=0A=

------=_NextPart_000_0001_01C36702.088C16E0
Content-Type: application/octet-stream;
	name="index"
Content-Transfer-Encoding: quoted-printable
Content-Disposition: attachment;
	filename="index"

#! /bin/bash=0A=
# Display an index of Axiom dependencies=0A=
# Bill Page=0A=
# 14 August 2003=0A=
#=0A=
export LC_ALL=3DC=0A=
depends int/algebra/*.spad | sort -u | unique=0A=

------=_NextPart_000_0001_01C36702.088C16E0
Content-Type: application/octet-stream;
	name="order"
Content-Transfer-Encoding: quoted-printable
Content-Disposition: attachment;
	filename="order"

#! /bin/bash
#  Use topological sort to determine a feasible order of
#  compilation. Loops are indicated by
#  tsort: x
#  tsort: y
#  tsort: loop=20
#  Bill Page
#  14 August 2003

export LC_ALL=3DC
depends int/algebra/*.spad | awk '{print $1" " $7;}' | sort -u | tsort =
2>&1 | tac

------=_NextPart_000_0001_01C36702.088C16E0--




