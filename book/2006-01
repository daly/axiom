++added:
\subsection{REBOOT}

<<reboot>>=
#!/bin/bash

#Un-mount all loop devices, including /
umount -lda

#Call system shutdown to halt system
sysshutdown -r now
@

\subsection{SHUTDOWN}

<<shutdown>>=
#!/bin/bash

#Un-mount all loop devices, including /
umount -lda

#Call system shutdown to halt system
sysshutdown -h now
@


