fallingPower (p,n) =3D=3D p*fallingPower(p-1,n-1)

test0 () =3D=3D
   n : Matrix Integer
   n :=3D new(3,3,0)
   for i in 1..3 repeat _
         for j in 1..3 repeat _
         n(i,j) :=3D fallingPower(i+j,j)
   n

test1 () =3D=3D
   n : Matrix Integer
   n :=3D new(3,3,0)
   for i in 1..3 repeat _
         for j in 1..3 repeat _
         n(i,j) :=3D i+j
   n

test2 () =3D=3D
   n : Matrix Integer =20
   n :=3D new(3,3,0)
   for i in 1..3 repeat _
         for j in 1..3 repeat _
         n(i,j) :=3D fallingPower(i,j)
   n


test2()
test1()
test0()
