Changes http://page.axiom-developer.org/zope/mathaction/231SimplifyWorkingOnMatrices/diff
--
\begin{axiom}
A:=matrix _
 [[tan(x)**6 + 3*tan(x)**4 + 3*tan(x)**2 + 1, _
   tan(x)**5 + 3*tan(x)**3 + 3*tan(x)], _
  [tan(x)**4 + 3*tan(x)**2, _
   tan(x)**3 + 3*tan(x)]]

map(simplify,A)
\end{axiom}
--
