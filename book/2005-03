\begin{axiom}
)abbrev package TEST1 Test1
++ Description: various Currying operations.
Test1(A:SetCategory):_
  MPcat == MPdef where
    B   ==>  Integer
 
    MPcat ==  with
        g:(A->B) -> (A ->B)
    MPdef == add
 
        MappingPackageInternalHacks3(A, B, B)
 
        fab:  A -> B
        sq: B -> B

        sq(x)== (x*x)$B
        g(fab) == comp(sq,fab,#1)
\end{axiom}

\begin{axiom}
p(x:INT):INT == x-1
p 4
\end{axiom}


\begin{axiom}
q := g p
q 4
\end{axiom}

